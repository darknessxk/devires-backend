{
    "name": "devires-backend",
    "version": "1.0.0",
    "private": true,
    "main": "build/index.js",
    "repository": "git@github.com:darknessxk/devires-backend.git",
    "author": "darknessxk <me@darknessxk.com>",
    "license": "MIT",
    "scripts": {
        "dev": "nodemon",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint:fix": "eslint --ext .ts src/ --fix",
        "prebuild": "yarn",
        "postbuild": "yarn doc",
        "build": "npm-run-all lint:fix test build:*",
        "build:ts": "tsc",
        "prestart": "yarn build",
        "start": "node .",
        "doc": "typedoc --exclude \"**/*+(jest|index|.spec|.test|.e2e).ts\" --name \"Devires Backend\"",
        "keys:generate": "./scripts/rsa-gen.sh",
        "prettify": "prettier --config .prettierrc.json --write src/**/*.ts",
        "coverage": "jest --coverage",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "password:hash": "ts-node ./scripts/hashPassword.ts",
        "database:new-user": "ts-node ./scripts/addUser.ts"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "class-validator": "^0.13.1",
        "cross-env": "^7.0.3",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "mysql": "^2.18.1",
        "node-jsonwebtoken": "^0.0.1",
        "npm-run-all": "^4.1.5",
        "reflect-metadata": "^0.1.13",
        "ts-node": "^9.1.1",
        "typedoc": "^0.20.34",
        "typeorm": "^0.2.31",
        "typescript": "^4.2.3"
    },
    "devDependencies": {
        "@jest/test-utils": "^0.0.0",
        "@jest/transform": "^26.6.2",
        "@tsconfig/recommended": "^1.0.1",
        "@types/express": "^4.17.11",
        "@types/faker": "^5.1.7",
        "@types/jest": "^26.0.22",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/node": "^14.14.36",
        "@types/supertest": "^2.0.10",
        "@typescript-eslint/eslint-plugin": "^4.19.0",
        "@typescript-eslint/parser": "^4.19.0",
        "eslint": "^7.22.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.3.1",
        "faker": "^5.5.0",
        "husky": "^5.2.0",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "pretty-quick": "^3.1.0",
        "sqlite3": "^5.0.2",
        "superagent": "^6.1.0",
        "supertest": "^6.1.3",
        "ts-jest": "^26.5.4",
        "typedoc-plugin-openapi-doc": "^0.3.1"
    }
}
