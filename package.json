{
    "name": "devires-backend",
    "version": "1.0.0",
    "private": true,
    "main": "build/index.js",
    "repository": "git@github.com:darknessxk/devires-backend.git",
    "author": "darknessxk <me@darknessxk.com>",
    "license": "MIT",
    "scripts": {
        "dev": "nodemon",
        "test": "jest",
        "lint:fix": "eslint --ext .ts src/ --fix",
        "prebuild": "yarn",
        "postbuild": "yarn doc",
        "build": "npm-run-all lint:fix test build:*",
        "build:ts": "tsc",
        "prestart": "yarn build",
        "start": "node .",
        "doc": "typedoc --name \"Devires Backend\"",
        "keys:generate": "./scripts/rsa-gen.sh",
        "prettify": "prettier --config .prettierrc.json --write src/**/*.ts"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "dependencies": {
        "cross-env": "^7.0.3",
        "express": "^4.17.1",
        "jose": "^3.11.1",
        "mysql": "^2.18.1",
        "npm-run-all": "^4.1.5",
        "reflect-metadata": "^0.1.13",
        "ts-node": "^9.1.1",
        "typedoc": "^0.20.34",
        "typeorm": "^0.2.31",
        "typescript": "^4.2.3"
    },
    "devDependencies": {
        "@jest/test-utils": "^0.0.0",
        "@jest/transform": "^26.6.2",
        "@tsconfig/recommended": "^1.0.1",
        "@types/express": "^4.17.11",
        "@types/jest": "^26.0.22",
        "@types/node": "^14.14.36",
        "@typescript-eslint/eslint-plugin": "^4.19.0",
        "@typescript-eslint/parser": "^4.19.0",
        "eslint": "^7.22.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.3.1",
        "husky": "^5.2.0",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "pretty-quick": "^3.1.0",
        "ts-jest": "^26.5.4"
    }
}
